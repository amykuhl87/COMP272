import java.security.*;
import java.util.*;
import java.io.*;

public class myHashSet {

    
    
    public ArrayList<String> listCreator(){
        ArrayList<String> words = new ArrayList<>();
        String line;

        //doing before I'm timing anything
        try{
            Scanner s = new Scanner(new File("C:\\COMP170\\IntelliJ Java Programs\\_my_Java_programs\\src\\EnglishWordList.txt"));
            while (s.hasNext()) {
                words.add(s.next());
            }
            s.close();
        } catch (Exception e){
            e.printStackTrace();
        }


        Collections.sort(words);

        for(int i = 0; i < words.size() - 1; i++){
            String current = words.get(i);
            String next = words.get(i + 1);
            if(current.compareTo(next) == 0 || next.length() == 0){
                words.remove(i + 1);
                i--;
            }
        }

        return words;
    }



    public int H1p95(String s){
        
        
        return Math.abs(s.hashCode()) % 262127;
    }

    public int H1p50(String s){


        return Math.abs(s.hashCode()) % 524288;
    }

    public int H2p95(String s){
        int value = 0;


        return value;
    }
    
    public int H2p50(String s){
